<nav class="sheet-tabs tabs" aria-role="{{localize 'SHEETS.FormNavLabel'}}">
    {{! Tabs nav }}
    {{#each tabs as |tab|}}
        <a class="flexrow {{tab.id}} {{tab.cssClass}}" data-action="tab" data-group="{{tab.group}}"
            data-tab="{{tab.id}}" data-tooltip="{{tab.label}}" data-tooltip-class="cog" data-tooltip-direction="RIGHT">
            <div class="tab-icon" style="mask-image: url({{tab.icon}})"></div>
        </a>
    {{/each}}
</nav>
