{
  "id": "cog",
  "title": "Chroniques Oubliées Galactiques",
  "description": "Chroniques Oubliées Galactiques game system.",
  "version": "0.0.1",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "url": "https://foundryvtt.com/packages/cog",
